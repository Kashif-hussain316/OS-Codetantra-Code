#include <stdio.h>

    printf("Enter number of frames: ");
    scanf("%d", &frames);

    int frame[frames];
    int last_used[frames];
    for (i = 0; i < frames; i++) {
        frame[i] = -1;
        last_used[i] = -1;
    }

    int page_faults = 0;

    for (i = 0; i < n; i++) {
        int found = 0;
        for (j = 0; j < frames; j++) {
            if (frame[j] == pages[i]) {
                found = 1;
                last_used[j] = i;
                break;
            }
        }

        if (!found) {
            page_faults++;
            int lru_index = 0;
            int min_last_used = 1e9;

            for (j = 0; j < frames; j++) {
                if (frame[j] == -1) {
                    lru_index = j;
                    break;
                }
                if (last_used[j] < min_last_used) {
                    min_last_used = last_used[j];
                    lru_index = j;
                }
            }

            frame[lru_index] = pages[i];
            last_used[lru_index] = i;
        }
    }

    printf("Total Page Faults = %d\n", page_faults);
    return 0;
}
