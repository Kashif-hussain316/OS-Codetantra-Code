#include <stdio.h>
    printf("Enter number of frames: ");
    scanf("%d", &frames);

    int frame[frames];
    for (i = 0; i < frames; i++) frame[i] = -1;

    int page_faults = 0;

    for (i = 0; i < n; i++) {
        int found = 0;
        for (j = 0; j < frames; j++) {
            if (frame[j] == pages[i]) {
                found = 1;
                break;
            }
        }
        if (!found) {
            page_faults++;

            int index_to_replace = -1, farthest = -1;
            for (j = 0; j < frames; j++) {
                if (frame[j] == -1) {
                    index_to_replace = j;
                    break;
                }
                int next_use = n + 1;
                for (k = i + 1; k < n; k++) {
                    if (frame[j] == pages[k]) {
                        next_use = k;
                        break;
                    }
                }
                if (next_use > farthest) {
                    farthest = next_use;
                    index_to_replace = j;
                }
            }
            frame[index_to_replace] = pages[i];
        }
    }

    printf("Total Page Faults = %d\n", page_faults);
    return 0;
}
